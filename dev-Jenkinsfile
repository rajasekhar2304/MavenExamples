pipeline {
    agent any
    

    stages {
        stage("SCM Checkout") {
            steps {
                git branch: 'main',
                    credentialsId: "github_access",                
                    url: "https://github.com/rajasekhar2304/MavenExamples.git"            
            }
        }
        stage('Build') {
            steps {
                sh "mvn clean install"
                
            }
        }
    }
}